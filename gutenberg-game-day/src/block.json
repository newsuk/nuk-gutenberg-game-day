{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/times-quiz-question",
	"version": "0.1.0",
	"title": "Quiz Question",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"label": {
			"type": "boolean",
			"default": false
		},
		"question": {
			"type": "string"
		},
		"choices": {
			"type": "array",
			"default": []
		},
		"answer": { "type": "number" },
		"details": { "type": "string" }
	},
	"textdomain": "times-quiz-question",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
