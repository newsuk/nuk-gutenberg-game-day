{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/gutenberg-game-day",
  "version": "0.1.0",
  "title": "Gutenberg Game Day",
  "category": "widgets",
  "icon": "smiley",
  "description": "Example block scaffolded with Create Block tool.",
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "gutenberg-game-day",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js",
  "attributes": {
    "kicker": {
      "type": "string",
      "default": "Kicker Text Here"
    },
    "kickerBackgroundColor": {
      "type": "string",
      "default": "#FFF"
    },
    "kickerTextColor": {
      "type": "string",
      "default": "#000"
    },
    "headline": {
      "type": "string",
      "default": "Headline Text Here"
    },
    "headlineTextColor": {
      "type": "string",
      "default": "#000"
    },
    "subdeck": {
      "type": "string",
      "default": "Subdeck Text Here"
    },
    "subdeckTextColor": {
      "type": "string",
      "default": "#000"
    },
    "kickerMaxLength": {
      "type": "integer",
      "default": 20
    },
    "headlineMaxLength": {
      "type": "integer",
      "default": 80
    },
    "subdeckMaxLength": {
      "type": "integer",
      "default": 120
    }
  }
}