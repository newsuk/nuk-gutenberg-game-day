{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "team9/fancy-headline-with-kicker-headline-subdeck",
	"version": "0.1.0",
	"title": "Fancy Headline with Kicker and Subdeck",
	"category": "text",
	"icon": "text",
	"description": "A block with a kicker, headline, and subdeck with customizable color settings.",
	"example": {},
	"supports": {
		"align": true,
		"alignWide": true,
		"html": false,
		"spacing": {
			"margin": true,
			"padding": true
		}
	},
	"attributes": {
		"text": {
			"type": "string",
			"default": "Your Kicker"
		},
		"color": {
			"type": "string",
			"default": "#000000"
		},
		"backgroundColor": {
			"type": "string",
			"default": "#FFFFFF"
		},
		"headline": {
			"type": "string",
			"default": "Your Headline"
		},
		"headlineColor": {
			"type": "string",
			"default": "#000000"
		},
		"subdeck": {
			"type": "string",
			"default": "Your Subdeck"
		},
		"subdeckColor": {
			"type": "string",
			"default": "#000000"
		}
	},
	"textdomain": "fancy-headline-with-kicker-headline-subdeck",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
