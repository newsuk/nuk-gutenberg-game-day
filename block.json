{
	"apiVersion": 2,
	"name": "blocks-course/eyecatcher",
	"title": "Eye Catcher",
	"category": "text",
	"icon": "text-page",
	"description": "A box of text and magic!",
	"keywords": ["text", "paragraph", "box"],
	"supports": {
		"html": false,
		"color": {
			"background": true
		}
	},
	"textdomain": "eyecatcher",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css",
	"attributes": {
		"kicker": {
			"type": "string",
			"source": "html",
			"selector": "h2"
		},
		"headline": {
			"type": "string",
			"source": "html",
			"selector": "h1",
			"style": {
				"type": "object",
				"default": {
					"color": {
						"background": "#b50000"
					}
				}
			}
		},
		"subdeck": {
			"type": "string",
			"source": "html",
			"selector": "p"
		}
	}
}
