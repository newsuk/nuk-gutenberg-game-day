{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/plotpoint",
	"version": "0.1.0",
	"title": "Plotpoint",
	"category": "widgets",
	"icon": "chart-line",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"chartType": {
			"type": "string",
			"default": "line"
		},
		"chartRows": {
			"type": "number"
		},
		"chartTitle": {
			"type": "string"
		},
		"dataSetTitle": {
			"type": "string"
		},
		"chartData": {
			"type": "array",
			"default": []
		},
		"chartOptions": {
			"type": "object",
			"default": {}
		}
	},
	"textdomain": "plotpoint",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
